{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "pfincher99/ahod_server1:dev",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/imapex/ahod/iot-server-dev",
    "instances": 1,
    "cpus": 0.1,
    "mem": 16,
    "env": {
        "SPARK_TOKEN":"$spark_token",
        "SPARK_ROOM":"$spark_room"
    }
}
